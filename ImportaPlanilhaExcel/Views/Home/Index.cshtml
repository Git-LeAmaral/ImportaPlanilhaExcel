@{
    ViewData["Title"] = "Home Page";
}

<div class="container mt-5">
    <form method="post" asp-action="ImportExcel" asp-controller="Home" enctype="multipart/form-data" class="text-center d-flex flex-column">
        <div class="input-group">
            <input type="file" name="form" class="form-control" />
            <button class="btn btn-secondary" type="submit">Importar Excel</button>
        </div>
    </form>

    <table class=table "table-striped table-hover mt-5">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nome</th>
                <th>Valor</th>
                <th>Quantidade</th>
                <th>Marca</th>
            </tr>
        </thead>

        <tbody>
            <tr>
                <td>Id</td>
                <td>Nome</td>
                <td>Valor</td>
                <td>Quantidade</td>
                <td>Marca</td>
            </tr>
        </tbody>
    </table>
</div>
